[user]
	name = Sam Figueroa
	email = unimatrixzxero@gmail.com
[color]
	ui = auto
	interactive = auto
[alias]
	st = status
	ci = commit
	df = diff --ignore-blank-lines -w
	gr = log --graph
	co = checkout
	br = branch
	cp = cherry-pick
	sh = show
	lol = log --pretty=oneline --abbrev-commit --graph --decorate
[core]
	excludesfile = /Users/neo/.gitignore
	quotepath = false
[push]
	default = tracking
[github]
	user = unimatrixzxero
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[init]
	templatedir = ~/.git/hooks
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
